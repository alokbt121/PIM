                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	pfc.ID	as ProductFamilyChannelID,
		pfc.ProductID,
		p.NPProductCode,
		pfc.ProductFamilyID,
		pf.Family as ProdFamily,
		pfc.ChannelID,
		dc.Channel,
		pfc.StatusID as ItemStatusID,
		ist.[Status] as ItemStatus,
		pfc.TNS,
		pfc.PLM,
		pfc.IRL,
		fcsr.[Description] as FamilyChannelStatusReason,
		pf.[Version] as ProductFamilyDescription,
		pfc.JPN,
        pfc.CZE,
        pfc.ChangedBy,
        pfc.UpdateDate
from ProductFamilyChannels pfc (nolock)
	left join ProductFamilys pf (nolock)
		on pfc.ProductID = pf.ProductID
			and pfc.ProductFamilyID = pf.ID
	left join DistributionChannels dc (nolock)
		on pfc.ChannelID = dc.ID
	left join ItemStatuses ist (nolock)
		on pfc.StatusID = ist.ID
	left join FamilyChannelStatusReason fcsr (nolock)
		on pfc.CommentID = fcsr.ID
	left join Products p (nolock)
		on pfc.ProductID = p.Id
	join 
		(
			select	pz.ProductID
			from ProductChannels pz
				join Products p
					on pz.ProductID = p.ID
			where	(
						pz.ItemStatusID in (3,5,6,7,8,12,13,14,15)
					)
			group by pz.ProductID
		) pc
	on pfc.ProductID = pc.ProductID</property>
