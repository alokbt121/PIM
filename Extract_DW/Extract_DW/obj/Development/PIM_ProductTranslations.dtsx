                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select pt.[ID]
      ,pt.[ProductID]
      ,pt.[LocaleID]
      ,l.[Locale]
      ,pt.[NPProductName]
      ,pt.[Name]
      ,pt.[Description]
      ,pt.[SubHeader]
      ,pt.[LongDescriptionCopy]
      ,pt.[WebName]
      ,pt.[Material]
      ,pt.[Keywords]
      ,pt.[NPurl]
      ,pt.[SEOTitle]
      ,pt.[SEODescription]
      ,pt.[SEOText]
      ,pt.[NPWebNotes]
      ,pt.[SageThemes]
      ,pt.[NPKeywords]
      ,pt.[ChannelID] 
from ProductTranslations Pt  (nolock) 
Join Locale L  (nolock) on Pt.LocaleID = L.ID
Join (Select P.ID ProductID
	  from Products P
	  Join ProductChannels PC on PC.ProductID = P.ID
	  where PC.ItemStatusID in (3,5,6,7,8,12,13,14,15)
	  ) Z on Z.ProductID = Pt.ProductID</property>
